sp {train-alarm
    (state <s> ^io <io>)
    (<io> ^output-link <out>)
    (<io> ^input-link <il>)
    (<il> ^training-vars <vars>)
    (<vars> ^name <name>)
    (<s> ^<name> <id>)
    (<vars> ^b <b>
            ^e <e>
            ^a <a>
            ^j <j>
            ^m <m>)
    -->
    (<out> ^training-ex <te>)
    (<te> ^id <id>)
    (<te> ^train <tr>)
    (<tr> ^boolean <b1> ^boolean <e1> ^boolean <a1> ^boolean <j1> ^boolean <m1>)
    (<b1> ^b <b>)
    (<e1> ^e <e>)
    (<a1> ^a <a>)
    (<j1> ^j <j>)
    (<m1> ^m <m>)
}

sp {train-traffic
    (state <s> ^io <io>)
    (<io> ^output-link <out>)
    (<io> ^input-link <il>)
    (<il> ^training-vars <vars>)
    (<vars> ^name <name>)
    (<s> ^<name> <id>)
    (<vars> ^r <r>
            ^t <t>
            ^l <l>)
    -->
    (<out> ^training-ex <te>)
    (<te> ^id <id>)
    (<te> ^train <tr>)
    (<tr> ^boolean <r1> ^boolean <t1> ^boolean <l1>)
    (<r1> ^r <r>)
    (<t1> ^t <t>)
    (<l1> ^l <l>)
}