sp {query-play
   (state <s> ^io <io>)
   (<io> ^output-link <out>)
   (<io> ^input-link <il>)
   (<il> ^query-signal <q>)
   (<q>  ^name <name>
         ^outlook <olook>
         ^temp <temp>
         ^humidity <humid>
         ^windy <windy>)
   (<s> ^<name> <id>)
-->
   (<out> ^query-handler <qh>)
   (<qh> ^id <id> ^query <q>)
   (<q> ^features <f>)
   (<f> ^categorical <o1> ^numerical <t1> ^numerical <h1> ^boolean <w1>)
   (<o1> ^outlook <olook>)
   (<t1> ^temp <temp>)
   (<h1> ^humidity <humid>)
   (<w1> ^windy <windy>)
   (<q> ^parameters <p>)
   (<p> ^k 1)
   (<p> ^distance euclidean)
}

sp {query-sign
   (state <s> ^io <io>)
   (<io> ^output-link <out>)
   (<io> ^input-link <il>)
   (<il> ^query-signal <q>)
   (<q>  ^name <name>
         ^shape <shape>
         ^color <color>)
   (<s> ^<name> <id>)
-->
   (<out> ^query-handler <qh>)
   (<qh> ^id <id> ^query <q>)
   (<q> ^features <f>)
   (<f> ^categorical <sh1> ^categorical <c1>)
   (<sh1> ^shape <shape>)
   (<c1> ^color <color>)
   (<q> ^parameters <p>)
   (<p> ^k 1)
   (<p> ^distance euclidean)
}