sp {train-play
    (state <s> ^io <io>)
    (<io> ^output-link <out>)
    (<io> ^input-link <il>)
    (<il> ^training <vars>)
    (<vars> ^name <name>)
    (<s> ^<name> <id>)
    (<vars> ^outlook <olook>
            ^temp <temp>
            ^humidity <humid>
            ^windy <windy>
            ^play <play>)
    -->
    (<out> ^training-ex <te>)
    (<te> ^id <id>)
    (<te> ^train <tr>)
    (<tr> ^categorical <o1> ^numerical <t1> ^numerical <h1> ^boolean <w1> ^categorical <p1>)
    (<o1> ^outlook <olook>)
    (<t1> ^temp <temp>)
    (<h1> ^humidity <humid>)
    (<w1> ^windy <windy>)
    (<p1> ^play <play>)
}

sp {train-sign
    (state <s> ^io <io>)
    (<io> ^output-link <out>)
    (<io> ^input-link <il>)
    (<il> ^training <vars>)
    (<vars> ^name <name>)
    (<s> ^<name> <id>)
    (<vars> ^shape <shape>
            ^color <color>
            ^sign <sign>)
    -->
    (<out> ^training-ex <te>)
    (<te> ^id <id>)
    (<te> ^train <tr>)
    (<tr> ^categorical <sh1> ^categorical <c1> ^categorical <si1>)
    (<sh1> ^shape <shape>)
    (<c1> ^color <color>)
    (<si1> ^sign <sign>)
}
